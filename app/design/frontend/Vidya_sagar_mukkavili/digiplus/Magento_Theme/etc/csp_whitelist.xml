<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>


        <policy id="style-src">
            <values>
                <value id="googleapis" type="host">*.googleapis.com</value>
                <value id="gstatic" type="host">*.gstatic.com*</value>
                <value id="fontawesome" type="host">*.fontawesome.com</value>
                <value id="fontawesomekit" type="host">https://kit.fontawesome.com/99632c8de8.js</value>
                <value id="fontawesomekit_02" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOmCnqEu92Fr1Mu7GxKOzY.woff2</value>
                <value id="fontawesomekit_03" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOlCnqEu92Fr1MmWUlfCRc4EsA.woff2</value>
                


            </values>
        </policy>

<!-- https://kit.fontawesome.com/99632c8de8.js -->


        <policy id="script-src">
            <values>
                <value id="devdocs-base" type="host">https://developer.adobe.com</value>
                <value id="magento" type="host">https://magento.com</value>
                <value id="googleapis" type="host">*.googleapis.com</value>
                <value id="gstatic" type="host">*.gstatic.com*</value>
                <value id="fontawesome" type="host">*.fontawesome.com</value>
                <value id="fontawesomekit" type="host">https://kit.fontawesome.com/99632c8de8.js</value>
  
            </values>
        </policy>
        
        <policy id="connect-src">
            <values>
                <value id="devdocs" type="host">https://developer.adobe.com</value>
                <value id="fontawesome" type="host">*.fontawesome.com</value>
                <value id="fontawesomekit" type="host">https://kit.fontawesome.com/99632c8de8.js</value>
                <value id="fontawesomekit_02" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOmCnqEu92Fr1Mu7GxKOzY.woff2</value>
                <value id="fontawesomekit_03" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOlCnqEu92Fr1MmWUlfCRc4EsA.woff2</value>
                
            </values>
        </policy>


                <policy id="font-src">
            <values>
                <value id="googleapis" type="host">https://fonts.googleapis.com</value>
                <!-- <value id="gstatic" type="host">https://fonts.gstatic.com</value> -->
                <value id="gstatic" type="host">*.gstatic.com*</value>
                <value id="fontawesome" type="host">*.fontawesome.com</value>
                <!-- <value id="magento" type="host">https://magento.com</value> -->
                <value id="fontawesomekit" type="host">https://kit.fontawesome.com/99632c8de8.js</value>
                <value id="fontawesomekit_02" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOmCnqEu92Fr1Mu7GxKOzY.woff2</value>
                <value id="fontawesomekit_03" type="host">https://fonts.gstatic.com/s/roboto/v30/KFOlCnqEu92Fr1MmWUlfCRc4EsA.woff2</value>
                
            </values>
        </policy>



    </policies>
</csp_whitelist>
